{
    "name": "dinkara/repobuilder",
    "type": "library",
    "description": "Create Migration,Model and Repository from console",
    "keywords": [
        "laravel",
        "repository pattern",
        "laravel repository pattern",
        "eloquent",
        "repository"
    ],
    "homepage": "https://github.com/dinkara/repobuilder",
    "license": "MIT",
    "authors": [
        {
            "name": "Dinkara",
            "email": "dinkicnikola@yahoo.com",
            "role": "Developer"
        },
        {
            "name": "Dzale",
            "email": "ndzakovic@yahoo.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0",
        "sofa/eloquence": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Dinkara\\RepoBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dinkara\\RepoBuilder\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dinkara\\RepoBuilder\\RepositoryBuilderServiceProvider"
            ]
        }
    }
}
